[user]
    name = Yusuke Hatanaka
    email = arbalestimp@gmail.com
[core]
    excludesfile = /Users/hatajoe/.gitignore
    quotepath = false
    preloadindex = true
	editor = vim -c \"set fenc=utf-8\"
[pager]
    log = diff-highlight | less
    show = diff-highlight | less
    diff = diff-highlight | less
[diff]
    patience = true
    histogram = true
[alias]
    cm = commit
    co = checkout
    rb = rebase
    pl = !git fetch --prune && git pull
    ps = push
    br = branch -a
    st = stash
    mg = merge
    ac = !git add -A && git commit
    ctags = !.git/hooks/ctags
    tr = log --graph --pretty='format:%C(yellow)%h%Creset %s %Cgreen(%an)%Creset %Cred%d%Creset'
[help]
    autocorrect = 1
[color]
    ui = 1
[init]
    templatedir = ~/.gittemp
[ghq]
    root = /Users/hatajoe/src
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[push]
	default = simple
[merge "mergepbx"]
	name = Xcode project files merger
	driver = mergepbx %O %A %B
