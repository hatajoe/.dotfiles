[user]
	name = Yusuke Hatanaka
	email = arbalestimp@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	quotepath = false
	preloadindex = true
	editor = vim -c \"set fenc=utf-8\"
[alias]
	cm = commit
	co = checkout
	rb = rebase
	pl = !git fetch --prune && git pull && git delete-merged-branches
	fe = !git fetch --prune && git delete-merged-branches
	ps = push
	br = branch
	ac = !git add -A && git commit
	delete-merged-branches = !git branch --merged | grep -vE '^\\*|master$|develop$|deploy-.*$' | xargs -I % git branch -d %
[push]
	default = simple
[submodule]
	recurse = true
[url "git@github.com:"]
	insteadOf = https://github.com/
[include]
	path = ~/.gitconfig.secret
