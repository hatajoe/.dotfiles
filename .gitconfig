[user]
	name = Yusuke Hatanaka
	email = arbalestimp@gmail.com
[core]
	excludesfile = /Users/hatajoe/.gitignore
	quotepath = false
	preloadindex = true
	editor = vim
[diff]
    patience=true
    histogram=true
[alias]
	cm = commit
	co = checkout
	ac = !git add -A && git commit
	st = status -sb
	ctags = !.git/hooks/ctags
[help]
	autocorrect = 1
[color]
	ui = 1
[init]
	templatedir = ~/.git_template
[branch "master"]
	rebase = true
[branch]
	autosetuprebase = always
[pull]
	rebase = true
[merge]
	ff = false
